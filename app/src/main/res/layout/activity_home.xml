<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity"
    android:background="@drawable/gradientbg">

    <TextView
        android:id="@+id/textView"
        android:layout_width="227dp"
        android:layout_height="58dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/poppinsbold"
        android:text="Your best collection of NFT art"
        android:textColor="@color/black"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="34dp"
        android:layout_marginEnd="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/baseline_notifications_24" />

    <TextView
        android:id="@+id/favcreator"
        android:layout_width="159dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/poppinsbold"
        android:text="Favorite Creator"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <FrameLayout
        android:id="@+id/frameLayoutRetro"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/kotak_radius"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/favcreator">

        <ImageButton
            android:id="@+id/imagebuttonretro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:layout_toLeftOf="@+id/imageButton2"
            android:background="@color/white"
            android:padding="1dp"
            android:src="@drawable/baseline_circle_30" />

        <Button
            android:id="@+id/textbuttonretro"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="35dp"
            android:layout_marginRight="5dp"
            android:background="@android:color/transparent"
            android:fontFamily="@font/poppinsregular"
            android:text="RetroNFT"
            android:textAllCaps="false"
            android:textColor="@color/black">

        </Button>

    </FrameLayout>


    <FrameLayout
        android:id="@+id/frameLayoutubah"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/kotak_radius"
        android:backgroundTint="#7B66FF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent">

        <ImageButton
            android:id="@+id/imagebuttonubah"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:layout_toLeftOf="@+id/imageButton2"
            android:backgroundTint="#304D30"
            android:padding="1dp"
            android:src="@drawable/baseline_circle_30" />

        <Button
            android:id="@+id/textbuttonubah"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="35dp"
            android:layout_marginRight="5dp"
            android:background="@android:color/transparent"
            android:fontFamily="@font/poppinsregular"
            android:text="Ubah"
            android:textAllCaps="false"
            android:textStyle="bold"
            android:textColor="@color/white">

        </Button>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayoutShark"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/kotak_radius"
        app:layout_constraintEnd_toStartOf="@+id/frameLayoutRacer"
        app:layout_constraintStart_toEndOf="@+id/frameLayoutRetro"
        app:layout_constraintTop_toBottomOf="@+id/favcreator">

        <ImageButton
            android:id="@+id/imagebuttonshark"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:layout_toLeftOf="@+id/imageButton2"
            android:background="@color/white"
            android:padding="1dp"
            android:src="@drawable/baseline_circle_30" />

        <Button
            android:id="@+id/textbuttonshark"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="35dp"
            android:layout_marginRight="5dp"
            android:background="@android:color/transparent"
            android:fontFamily="@font/poppinsregular"
            android:text="SharkNFT"
            android:textAllCaps="false"
            android:textColor="@color/black">

        </Button>

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayoutRacer"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/kotak_radius"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/favcreator">

        <ImageButton
            android:id="@+id/imagebuttonracer"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:layout_toLeftOf="@+id/imageButton2"
            android:background="@color/white"
            android:padding="1dp"
            android:src="@drawable/baseline_circle_30" />

        <Button
            android:id="@+id/textbuttonracer"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="35dp"
            android:layout_marginRight="5dp"
            android:background="@android:color/transparent"
            android:fontFamily="@font/poppinsregular"
            android:text="RacerNFT"
            android:textAllCaps="false"
            android:textColor="@color/black">

        </Button>

    </FrameLayout>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/poppinsbold"
        android:text="Popular Bid"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayoutRetro" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentbutton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5">

    </androidx.fragment.app.FragmentContainerView>

</androidx.constraintlayout.widget.ConstraintLayout>